        {
            "attr" : "highlight_interpolations",
            "default" : "1",
            "desc" : "0:None; 1:Linear; 2:Cubic; 3:Spline",
            "items" :
            [
                [ "0", "None", "None" ],
                [ "1", "Linear", "Linear" ],
                [ "2", "Cubic", "Cubic" ],
                [ "3", "Spline", "Spline" ]
            ]
        },
        {
            "attr": "highlight_preset",
            "default" : "0",
            "desc" : "0:Default; 1:Square; 2:Hexagon; 3:Star; 4:Heart",
