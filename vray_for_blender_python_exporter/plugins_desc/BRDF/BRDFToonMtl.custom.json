                ["1", "Square", "Square"],
                ["2", "Hexagon", "Hexagon"],
                ["3", "Star", "Star"],
                ["4", "Heart", "Heart"]
            ],
            "type" : "ENUM",
            "ui": {},
            "options": {
                "derived": true
            },
            "update": "updateRemapWidget"
        },
        {
            "attr" : "color_spline_type",
            "default" : "0",
            "desc" : "Color spline type to export. BRDFToonMtl only supports combined curves export.",
            "items" :
            [
                [ "0", "Combined", "Combined"]
            ],
            "type" : "ENUM",
            "ui" : {},
            "options": {
                "derived": true
            }
        },
        {
            "attr": "curves_data",
            "type": "STRING",
            "desc": "Internal representation of the remapping curves data",
            "default": "",
            "options": {
               "derived": true
            }
        },
        {
            "attr": "diffuse",
            "default": [
                1.0,
                1.0,
                1.0
            ]
        },
        {
            "attr": "opacity_source",
            "default": "1"
        },
        {
            "attr" : "opacity_mode",
            "default" : "2"
        },
        {
            "attr" : "roughness_model",
            "default" : "1"
        },
        {
            "attr": "self_illumination_gi",
            "default": false
        },
        {
            "attr": "fresnel",
            "default": true
        },
        {
            "attr": "highlight_rotation",
            "ui": {
                "spin_step": 1.0
            },
            "subtype": "ANGLE"
        },
        {
            "attr": "reflect_dim_distance",
            "default": 1.0,
            "subtype": "DISTANCE"
        },
        {
            "attr": "fog_mult",
            "ui": {
                "soft_min": 0.1,
                "soft_max": 10.0,
                "spin_step": 0.01
            },
            "subtype": "DISTANCE"
        },
        {
            "attr": "fog_color_colortex",
            "type": "COLOR_TEXTURE",
            "color_prop": "fog_color",
            "tex_prop": "fog_color_tex",
            "ui": {
                "display_name": "Fog color",
                "rollout": "Fog",
                "tab": "V-Ray Material Advanced"
            },
            "desc": "The absorption (fog) color",
            "default": [
                1.0,
                1.0,
                1.0
            ]
        },
        {
            "attr": "lineWidth_tex",
            "default": 1.0,
            "ui": {
                "soft_max": 100.0,
                "spin_step": 0.1
            }
        },
        {
            "attr": "innerLineControl",
            "default": false
