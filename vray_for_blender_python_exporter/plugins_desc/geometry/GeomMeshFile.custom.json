{
    "ID": "GeomMeshFile",
    "Parameters": [
        {
            "attr" : "previewType",
            "default" : "Preview",
            "desc" : "The type of the generated preview",
            "items" : 
            [
                [ "None", "None", "" ],
                [ "Full", "Full", "" ],
                [ "Preview", "Preview", "" ]
            ],
            "type" : "ENUM",
            "options": {
                "derived": true
            },
            "update": "onUpdatePreview"
        },
        {
            "attr": "isAlembic",
            "type": "BOOL",
            "desc": "True if the plugin references an Alembic file",
            "default": false,
            "options": {
               "derived": true
            },
            "get": "isAlembicGet"

        },
        {
            "attr": "scale",
            "default": 1.0,
            "ui": {
                "soft_min": -10.0,
                "soft_max": 10.0,
                "spin_step": 0.1
            }
        },
        {
            "attr" : "alembic_layers",
            "default" : "",
            "desc" : "Semicolon-separated list of Alembic files for layering",
            "type" : "STRING",
            "ui" : {}
        },
        {
            "attr": "num_preview_faces",
            "desc": "The number of faces in the currently visible preview. Cannot be changed for .vrmesh files"
        },
        {
            "attr": "file",
            "update": "onUpdatePreviewFile",
            "options": {
                "shadowed": true
            }
        },
        {
            "attr": "num_preview_faces",
            "update": "onUpdatePreview"
        },
        {
            "attr": "flip_axis",
            "update": "onUpdatePreview"
        }
    ],
    "Widget": {
        "widgets": [
            {
                "layout": "ROLLOUT",
                "name": "file",
                "label": "File",
                "default_closed": false,
                "attrs": [
                    {
                        "name": "file"
                    },
                    {
                        "layout": "SEPARATOR"
                    },
                    {
                        "name": "previewType"
                    },
                    {
                        "name": "num_preview_faces",
                        "active": {"cond": "::isAlembic!=0"},
                        "visible": {"cond": "::previewType=Preview"}
                    },
                    {
                        "layout": "SEPARATOR"
                    },
                    {
                        "layout": "COLUMN",
                        "name": "",
                        "label": "",
                        "attrs": []
                    },
                    {
                        "layout": "SEPARATOR"
                    },
                    {
                        "layout": "ROLLOUT",
