{
    "ID": "TexNoiseMax",
    "Name": "V-Ray Noise",
    "Parameters": [
        {
            "attr": "color1",
            "ui": {
                "display_name": "Color 1"
            }
        },
        {
            "attr": "color2",
            "ui": {
                "display_name": "Color 2"
            }
        },
        {
            "attr": "size",
            "default": 0.1,
            "ui": {
                "soft_min": 0.0,
                "soft_max": 1.0,
                "spin_step": 0.01
            }
        },
        {
            "attr": "phase",
            "ui": {
                "soft_min": -10.0,
                "soft_max": 10.0,
                "spin_step": 0.1
            }
        },
        {
            "attr": "uvwgen",
            "ui" : {
                "display_name": "Mapping"
            },
            "type": "UVWGEN"
        }
    ],
    "Widget": {
        "widgets": [
            {
                "attrs": [
                    {
                        "name": "type"
                    },
                    {
                        "name": "color1"
                    },
                    {
                        "name": "color2"
                    },
                    {
                        "name": "size"
                    },
                    {
                        "name": "iterations",
                        "active": {"cond": "::type!=0"}
                    },
                    {
                        "name": "phase"
                    }
                ],
                "layout": "COLUMN"
            },
            {
                "layout": "ROLLOUT",
                "name": "noise threshold",
                "label": "Noise Threshold",
                "default_closed": false,
                "attrs": [
                    {
                        "name": "low"
                    },
                    {
                        "name": "high"
                    }
                ]
            }
        ]
    },
    "Node": {
        "input_sockets": [
            { "name": "size" },
            { "name": "phase" },
            { "name": "iterations" },
            { "name": "low" },
            { "name": "high" },
            { "name": "uvwgen" }
        ]
    },
    "Options": {
        "animatable": true,
        "excluded_parameters": [
            "compatibility_with",
            "alpha_from_intensity",
            "invert",
            "invert_alpha",
            "color_mult",
            "color_offset",
            "alpha_mult",
            "alpha_offset",
            "nouvw_color",
            "color",
            "placement_type",
            "u",
            "v",
            "w",
            "h",
            "jitter",
            "tile_u",
            "tile_v",
            "uv_noise_on",
            "uv_noise_animate",
            "uv_noise_amount",
            "uv_noise_levels",
            "uv_noise_size",
            "un_noise_phase"
        ]
    }
}